    /**
     * Creates new form ACTION1
     */
    String bmi;
    String olahraga;
    String gender;
    String tinggi1;
    String berat1;
    String usia1;
    public ACTION1(String bmi, String olahraga, String gender, String tinggi1, String berat1, String usia1) {
        initComponents();
        this.bmi = bmi;
        tampilbmi.setText(this.bmi); 
        this.olahraga = olahraga;
        tampilolahragaACT.setText(olahraga);
        this.gender = gender;
        tampilgender.setText(gender);
        this.tinggi1 = tinggi1;
        this.berat1 = berat1;
        this.usia1 = usia1;
        
        double d = Double.valueOf(bmi);
        double a = Double.valueOf(olahraga);
        double g = Double.valueOf(gender);
        double t = Double.valueOf(tinggi1);
        double b = Double.valueOf(berat1);
        double u = Double.valueOf(usia1);
        
        double kali = 66.5+(13.75*b)+(5.003*(t*100)-(6.75*u));
        double kali2 = 655.1+(9.563*b)+(1.850*(t*100)-(4.676*u));
        //buat bikin kalori harian//
        if (g==1){
            if(a<1){
                double ngitung = kali*1.2;
                String ngitungco = String.format("%.2f",ngitung);
                tampilkalori.setText(ngitungco);
            }else if(a>=1 && a<=2){
                double ngitung = kali*1.3;
                String ngitungco = String.format("%.2f",ngitung);
                tampilkalori.setText(ngitungco);
            }else if(a>2 && a<=5){
                double ngitung = kali*1.4;
                String ngitungco = String.format("%.2f",ngitung);
                tampilkalori.setText(ngitungco);
            }else if(a>5 && a>=7){
                double ngitung = kali*1.5;
                String ngitungco = String.format("%.2f",ngitung);
                tampilkalori.setText(ngitungco);
            }
        } else if(g==0){
            if(a<1){
                double ngitung2 = kali2*1.2;
                String ngitungce = String.format("%.2f",ngitung2);
                tampilkalori.setText(ngitungce);
            }else if(a>=1 && a<=2){
                double ngitung2 = kali2*1.3;
                String ngitungce = String.format("%.2f",ngitung2);
                tampilkalori.setText(ngitungce);
            }else if(a>2 && a<=5){
                double ngitung2 = kali2*1.4;
                String ngitungce = String.format("%.2f",ngitung2);
                tampilkalori.setText(ngitungce);
            }else if(a>5 && a>=7){
                double ngitung2 = kali2*1.5;
                String ngitungce = String.format("%.2f",ngitung2);
                tampilkalori.setText(ngitungce);
            }
        }
        //buat bikin kondisi badan//
        if(d< 18.5){
            kondisi.setText("belom memenuhi kalori harian \nyang dibutuhkan");
        }else if(d>=18.5 && d<25){
            kondisi.setText("Sudah memenuhi kalori harian \nyang dibutuhkan");
        }else if(d>=25 && d<30){
            kondisi.setText("Terlalu banyak kalori \nyang dikonsumsi dalam sehari");
        }else if(d>=30 && d<40){
            kondisi.setText("sangat berlebihan mengkonsumsi \nkalori dalam sehari");
        }else if(d>=40){
            kondisi.setText("Anda mengkonsumsi kalori \nterlalu extreme dalam sehari");
        }
        
        if(a<1){
            double kaliyuk = a + 3;
            String melehoy = String.format("%.2f",kaliyuk);
            saranolga.setText("Ditambah"+melehoy+"x");
        }else if(a>=1 && a<=2){
            double kaliyuk = a + 1;
            String melehoy = String.format("%.2f",kaliyuk);
            saranolga.setText("Ditambah"+melehoy+"x");
        }else if(a>2 && a<=5){
            double kaliyuk = a + 0;
            String melehoy = String.format("%.2f",kaliyuk);
            saranolga.setText("Sangat Bagus \nOlahraga"+melehoy+"x Seminggu");
        }else if(a>5 && a>=7){
            double kaliyuk = a + 0;
            String melehoy = String.format("%.2f",kaliyuk);
            saranolga.setText("Lebih dari Cukup \nOlahraga"+melehoy+"x Seminggu");
        }
        
        //membuat saran pola makan//
        if(d < 18.5){ //kurus
            if(a<1){ //kurus gapernah olahraga
                sarpol.setText("Atur pola makan sehat untuk \nmemenuhi kebutuhan kalori harian");
            }else if(a>=1 && a<=2){ //kurus olga 1-2
                sarpol.setText("Pilih pola makan yang \nbisa memenuhi kalori harian");
            }else if(a>2 && a<=5){ //kurus olga 2-5
                sarpol.setText("Atur pola makan sehat untuk \nmemenuhi Aktivitas tinggi");
            }else if(a>5 && a>=7){ //kurus olga 5-7
                sarpol.setText("perbanyak konsumsi protein \nuntuk menambah masa otot");
            }
        }else if(d>=18.5 && d<25){ //normal
            if(a<1){ //normal gapernah olga
                sarpol.setText("Jaga Pola makan Anda");
            }else if(a>=1 && a<=2){ //normal olga 1-2
                sarpol.setText("Jaga Pola makan Anda");
            }else if(a>2 && a<=5){ //normal olga 2-5
                sarpol.setText("Jaga Pola makan Anda");
            }else if(a>5 && a>=7){ //normal olga 5-7
                sarpol.setText("Jaga Pola makan Anda");
            }
        }else if(d>=25 && d<30){
            if(a<1){
                sarpol.setText("Perhatikan pola makan Anda");
            }else if(a>=1 && a<=2){
                sarpol.setText("Pilihlah makanan yang kaya akan \nserat,vitamin, dan protein");
            }else if(a>2 && a<=5){
                sarpol.setText("pilih pola makan Anda \nyang sekiranya tidak berlebihan");
            }else if(a>5 && a>=7){
                sarpol.setText("pilih makanan yang berprotein \ndan jangan berlebihan");
            }
        }else if(d>=30 && d<40){
            if(a<1){
                sarpol.setText("Atur pola makan sehat untuk \nmemenuhi kalori harian saja");
            }else if(a>=1 && a<=2){
                sarpol.setText("Atur pola makan sehat untuk \nmemenuhi kalori harian saja");
            }else if(a>2 && a<=5){
                sarpol.setText("Pilihlah makanan yang kaya akan \nserat,vitamin, dan protein");
            }else if(a>5 && a>=7){
                sarpol.setText("Atur pola makan sehat untuk \nmemenuhi kalori harian saja."
                 + "\nSerta pilihlah olahraga \nyang menambah masa otot");
            }
        }else if(d>=40){
            if(a<1){
                sarpol.setText("Pilih program diet \nsesuai anjuran dokter");
            }else if(a>=1 && a<=2){
                sarpol.setText("Pilih program diet \nsesuai anjuran dokter");
            }else if(a>2 && a<=5){
                sarpol.setText("Pilih program diet \nsesuai anjuran dokter");
            }else if(a>5 && a>=7){
                sarpol.setText("Pilih program diet \nsesuai anjuran dokter");
            }
    }
    }

